[project]
title = Infinite Map
custom_resources = 

[bootstrap]
main_collection = /infinite_map/infinite_map.collectionc
render = /builtins/render/default.renderc

[input]
game_binding = /input/game.input_bindingc
gamepads = /builtins/input/default.gamepadsc

[display]
width = 1136
height = 640
variable_dt = 1

[physics]
scale = 0.02
debug = 0
max_collisions = 256
max_contacts = 256
gravity_y = -200.0

[script]
shared_state = 1

[android]
iap_provider = GooglePlay
package = com.britzl.publicexamples.screen_shake

[sprite]
max_count = 32000

[resource]
max_resources = 3000

[collection]
max_instances = 32000

[graphics]
max_debug_vertices = 50000

[osx]
bundle_identifier = com.britzl.publicexamples.screen_shake

[ios]
bundle_identifier = com.britzl.publicexamples.screen_shake

